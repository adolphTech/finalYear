      <th class="fs-6">Prescription Date.</th>
      <th class="fs-6">Medication</th>
      <th class="fs-6">Description</th>
      <th class="fs-6">Doctor: Name</th>
    </tr>
  </thead>
  <tbody>
    
    {{#each prescriptions}}
    <tr>
      <td>{{this.prescriptionDate}}</td>
      <td class="fs-6 text-nowrap">{{this.medication}}</td>
      <td>{{this.description}}</td>
      <td class="fs-6 text-nowrap">{{this.doctor.name}}</td>
      
    </tr>
    {{/each}}
  </tbody>
</table>
